{
  "name": "primitive",
  "version": "1.0.0",
  "description": "Design System Primitives using Styled Components",
  "main": "index.js",
  "scripts": {
    "start": "styleguidist server --config site/styleguide.config.js",
    "docs": "styleguidist build --config site/styleguide.config.js",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "lint": "eslint ./src",
    "fix": "eslint ./src --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebSeed/primitive.git"
  },
  "keywords": [
    "primitive",
    "css-in-js",
    "styled-components",
    "styling",
    "react",
    "design-system"
  ],
  "author": "Will Bamford",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WebSeed/primitive/issues"
  },
  "homepage": "https://github.com/WebSeed/primitive#readme",
  "jest": {
    "roots": [
      "./src"
    ],
    "setupFiles": [
      "./src/__tests__/__setup__/enzyme.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__mocks__/",
      "/__setup__/"
    ]
  },
  "peerDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-styleguidist": "^8.0.0",
    "react-test-renderer": "^16.6.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-node-resolve": "^3.4.0",
    "styled-components": "^4.0.2",
    "webpack": "^4.23.0"
  },
  "dependencies": {
    "prop-types": "^15.6.2"
  }
}
